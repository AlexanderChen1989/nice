<h2>Show cat</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @cat.name %>
  </li>

  <li>
    <strong>Dog:</strong>
    <%= @cat.dog_id %>
  </li>

  <li>
    <strong>Pig:</strong>
    <%= @cat.pig_id %>
  </li>

</ul>

<%= link "Edit", to: cat_path(@conn, :edit, @cat) %>
<%= link "Back", to: cat_path(@conn, :index) %>
